SELECT *
FROM 
 "BRIGHT_COFFEESHOP_SALES"."PUBLIC"."COFFEE";

SELECT MIN(TRANSACTION_TIME)
FROM 
 "BRIGHT_COFFEESHOP_SALES"."PUBLIC"."COFFEE";
 
SELECT MAX(TRANSACTION_TIME)
FROM 
 "BRIGHT_COFFEESHOP_SALES"."PUBLIC"."COFFEE";

 SELECT SUN(PRODUCT_ID) AS PRODUCT_SUM, PRODUCT_CATEGORY
FROM 
 "BRIGHT_COFFEESHOP_SALES"."PUBLIC"."COFFEE"
 GROUP BY 2;


 
 SELECT 
       TO_CHAR(TO_DATE(transaction_date, 'YYYY/MM/dd'), 'YYYY/MM') AS Month_ID,
       COUNT(PRODUCT_ID) AS product_sold,
       COUNT(TRANSACTION_ID) AS Unique_ID,
       SUM(TRANSACTION_QTY * TO_NUMBER(REPLACE(UNIT_PRICE,',','.'))) AS Total_revenue,
       store_location,
       store_id,product_category,
CASE 
    WHEN TRANSACTION_TIME BETWEEN '06:00:00' AND '11:59:59' THEN 'Morning'
    WHEN TRANSACTION_TIME BETWEEN '12:00:00' AND '17:59:59' THEN 'Afternoon'
    WHEN TRANSACTION_TIME BETWEEN '18:00:00' AND '19:59:59' THEN 'Evening'
    ELSE 'Night'
    END AS Time_Interval
FROM
  "BRIGHT_COFFEESHOP_SALES"."PUBLIC"."COFFEE"
  GROUP BY ALL
  ORDER BY total_revenue DESC;
